Количество = НужнаяТЗ1.Количество() - 1; 
		
		Для Индекс = 1 По Количество Цикл
			
			стр = НужнаяТЗ1[Количество-Индекс];
			Если стр.ПрогнозПродаж = 0 
				И стр.ПроцентВыполненияСуммыЧека = 0
				И стр.ПроцентВыполненияДлиныЧека = 0
				И стр.ПрогнозУслуги = 0
				И стр.ПрогнозПродажАксессуарыМобильные = 0 Тогда
				НужнаяТЗ1.Удалить(стр);
			КонецЕсли;	
		 
КонецЦикла;



